{
  "window.zoomLevel": 2,
  "[vue]": {
    "editor.defaultFormatter": "johnsoncodehk.volar"
  },
  "vetur.validation.template": false,
  "vetur.validation.interpolation": false,

  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "vue",
    "javascript",
    "javascriptreact",
    "html",
    "css",
    "scss",
    "less",
    "typescript"
  ],
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.options": {
    "overrideConfig": {
      "env": {
        "browser": true,
        "es6": true
      },
      "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "rules": {
        "no-debugger": "off"
      }
    }
  },
  "scssFormatter.printWidth": 80,
  "vue3snippets.printWidth": 80,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "rvest.vs-code-prettier-eslint",
  "vue3snippets.useTabs": true,
  "editor.insertSpaces": false,
  "javascript.validate.enable": false,
  "editor.formatOnType": true,
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "files.autoSave": "afterDelay",
  "vue3snippets.enable-compile-vue-file-on-did-save-code": true,
  "vue3snippets.vueIndentScriptAndStyle": true,
  "prettier.useTabs": false,
	"prettier.tabWidth": 2,
	"files.autoSaveDelay": 3000,
	"javascript.format.insertSpaceAfterCommaDelimiter": false,
	"typescript.format.insertSpaceAfterCommaDelimiter": false,
	"prettier.trailingComma": "all",
	"javascript.preferences.quoteStyle": "double",
	"typescript.preferences.quoteStyle": "double",
	"gitlens.views.fileHistory.avatars": false
}
